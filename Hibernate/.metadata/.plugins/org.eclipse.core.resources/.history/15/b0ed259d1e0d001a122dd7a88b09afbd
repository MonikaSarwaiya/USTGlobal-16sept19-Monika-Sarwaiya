package com.ustglobal.jpawithhibernateapp.manytomany;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Data
@Entity
@Table(name="course")
public class Course {

	@Column
	@Id
	private int cid;
	@Column
	private String cname;
}
