package com.ustglobal.jpawithhibernateapp.onetoone;

import javax.persistence.*;

@Entity
@Table(name="person")
public class VoterCard {

	@Column
	@Id
	private int pid;
	@Column
	private String vid;
	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public String getVid() {
		return vid;
	}
	public void setVid(String vid) {
		this.vid = vid;
	}
}
